{
  "overrides": [
    {
      "files": "*.dart",
      "options": {
        "printWidth": 80,
        "tabWidth": 2,
        "singleQuote": true,
        "trailingComma": "none",
        "endOfLine": "auto"
      }
    },
    {
      "files": "*.yaml",
      "options": {
        "printWidth": 80,
        "tabWidth": 2,
        "singleQuote": false,
        "trailingComma": "none",
        "endOfLine": "auto"
      }
    }
  ]
}
